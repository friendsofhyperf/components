{
    "name": "friendsofhyperf/command-signals",
    "description": "The signals component for Hyperf Command.",
    "keywords": [
        "hyperf",
        "v3.1"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "hyperf/context": "~3.1.0",
        "hyperf/engine": "^2.0",
        "hyperf/support": "~3.1.0"
    },
    "suggest": {
        "ext-swoole": ">=4.6.0",
        "ext-swow": ">=0.1.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\CommandSignals\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\CommandSignals\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    },
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    }
}
