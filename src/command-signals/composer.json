{
    "name": "friendsofhyperf/command-signals",
    "description": "The signals component for Hyperf Command.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "hyperf/context": "~3.0.16",
        "hyperf/engine": "^1.3.4|^2.0",
        "hyperf/support": "~3.0.1"
    },
    "suggest": {
        "ext-swoole": ">=4.6.0",
        "ext-swow": ">=0.1.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\CommandSignals\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\CommandSignals\\ConfigProvider"
        }
    },
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    }
}
