{
    "name": "friendsofhyperf/easy-command",
    "description": "The easy command component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    },
    "require": {
        "hyperf/collection": "~3.1.0",
        "hyperf/command": "~3.1.0",
        "hyperf/context": "~3.1.0",
        "hyperf/di": "~3.1.0",
        "hyperf/event": "~3.1.0",
        "hyperf/stringable": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "hyperf/tappable": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\EasyCommand\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\EasyCommand\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    }
