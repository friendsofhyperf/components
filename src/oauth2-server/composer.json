{
    "name": "friendsofhyperf/oauth2-server",
    "description": "The Oauth2 server for hyperf.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "hyperf",
        "oauth2",
        "v3.1",
        "oauth2-server"
    ],
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        },
        {
            "name": "zds"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components",
        "docs": "https://hyperf.fans",
        "pull-request": "https://github.com/friendsofhyperf/components/pulls"
    },
    "require": {
        "ext-openssl": "*",
        "league/oauth2-server": "^9.2",
        "hyperf/di": "~3.2.0",
        "hyperf/db-connection": "~3.2.0",
        "hyperf/command": "~3.2.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Oauth2\\Server\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.1-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\Oauth2\\Server\\ConfigProvider"
        }
    }
}
