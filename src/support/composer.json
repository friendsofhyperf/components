{
    "name": "friendsofhyperf/support",
    "description": "Another support component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "hyperf/di": "~3.0.0",
        "hyperf/collection": "~3.0.0",
        "hyperf/context": "~3.0.0",
        "hyperf/macroable": "~3.0.0",
        "hyperf/stringable": "~3.0.0",
        "hyperf/support": "~3.0.0",
        "hyperf/tappable": "~3.0.0",
        "laravel/serializable-closure": "^1.0",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Support\\": "src"
        }
    },
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\Support\\ConfigProvider"
        }
    },
    "suggest": {
        "hyperf/async-queue": "Required to use Hyperf\\AsyncQueue\\Job.(~3.0.0)"
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
