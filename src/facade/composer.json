{
    "name": "friendsofhyperf/facade",
    "description": "The facade component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    },
    "require": {
        "hyperf/di": "~3.0.0"
    },
    "suggest": {
        "hyperf/amqp": "Required to use (~3.0.0)",
        "hyperf/async-queue": "Required to use (~3.0.0)",
        "hyperf/cache": "Required to use (~3.0.0)",
        "hyperf/config": "Required to use (~3.0.0)",
        "hyperf/filesystem": "Required to use (~3.0.0)",
        "hyperf/framework": "Required to use (~3.0.0)",
        "hyperf/kafka": "Required to use (~3.0.0)",
        "hyperf/logger": "Required to use (~3.0.0)",
        "hyperf/redis": "Required to use (~3.0.0)",
        "hyperf/session": "Required to use (~3.0.0)",
        "hyperf/translation": "Required to use (~3.0.0)",
        "hyperf/validation": "Required to use (~3.0.0)",
        "hyperf/view": "Required to use (~3.0.0)"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Facade\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\Facade\\ConfigProvider"
        }
    },
    "config": {
        "sort-packages": true
    }
}
