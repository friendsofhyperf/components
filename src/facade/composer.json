{
    "name": "friendsofhyperf/facade",
    "description": "The commonly used facades for Hyperf.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "hyperf",
        "v3.1"
    ],
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components",
        "docs": "https://hyperf.fans",
        "pull-request": "https://github.com/friendsofhyperf/components/pulls"
    },
    "require": {
        "friendsofhyperf/support": "~3.2.0",
        "hyperf/context": "~3.2.0",
        "hyperf/di": "~3.2.0"
    },
    "suggest": {
        "friendsofhyperf/encryption": "Required to use (~3.2.0)",
        "hyperf/amqp": "Required to use (~3.2.0)",
        "hyperf/async-queue": "Required to use (~3.2.0)",
        "hyperf/cache": "Required to use (~3.2.0)",
        "hyperf/config": "Required to use (~3.2.0)",
        "hyperf/filesystem": "Required to use (~3.2.0)",
        "hyperf/framework": "Required to use (~3.2.0)",
        "hyperf/kafka": "Required to use (~3.2.0)",
        "hyperf/logger": "Required to use (~3.2.0)",
        "hyperf/redis": "Required to use (~3.2.0)",
        "hyperf/session": "Required to use (~3.2.0)",
        "hyperf/translation": "Required to use (~3.2.0)",
        "hyperf/validation": "Required to use (~3.2.0)",
        "hyperf/view": "Required to use (~3.2.0)"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Facade\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.1-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\Facade\\ConfigProvider"
        }
    }
}
