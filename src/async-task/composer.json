{
    "name": "friendsofhyperf/async-task",
    "description": "The async task for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    },
    "require": {
        "hyperf/di": "~3.0.0",
        "hyperf/event": "~3.0.0",
        "hyperf/framework": "~3.0.0",
        "hyperf/logger": "~3.0.0",
        "hyperf/utils": "~3.0.0",
        "laravel/serializable-closure": "^1.0"
    },
    "autoload": {
        "files": [
            "src/Functions.php"
        ],
        "psr-4": {
            "FriendsOfHyperf\\AsyncTask\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\AsyncTask\\ConfigProvider"
        }
    }
}
