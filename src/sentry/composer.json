{
    "name": "friendsofhyperf/sentry",
    "description": "The sentry component for Hyperf.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "hyperf",
        "sentry",
        "v3.1"
    ],
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components",
        "pull-request": "https://github.com/friendsofhyperf/components/pulls"
    },
    "require": {
        "hyperf/command": "~3.1.0",
        "hyperf/context": "~3.1.0",
        "hyperf/coroutine": "~3.1.0",
        "hyperf/di": "~3.1.0",
        "hyperf/event": "~3.1.0",
        "hyperf/exception-handler": "~3.1.0",
        "hyperf/framework": "~3.1.0",
        "hyperf/http-server": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "hyperf/tappable": "~3.1.0",
        "sentry/sentry": "^4.4.0"
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the elasticsearch client (^7.0|^8.0).",
        "hyperf/amqp": "Required to use the amqp event (~3.1.9).",
        "hyperf/crontab": "Required to use the crontab event (~3.1.7).",
        "hyperf/database": "Required to use the crontab event (~3.1.0).",
        "phpmyadmin/sql-parser": "Required to use the sql parser (^5.9)."
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Sentry\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.1-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\Sentry\\ConfigProvider"
        }
    }
}
