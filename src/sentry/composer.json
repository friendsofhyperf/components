{
    "name": "friendsofhyperf/sentry",
    "description": "The sentry component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "sentry"
    ],
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Sentry\\": "src/"
        }
    },
    "require": {
        "friendsofhyperf/http-request-lifecycle": "^3.0.19",
        "hyperf/command": "~3.0.0",
        "hyperf/database": "~3.0.0",
        "hyperf/di": "~3.0.0",
        "hyperf/event": "~3.0.0",
        "hyperf/exception-handler": "~3.0.0",
        "hyperf/framework": "~3.0.0",
        "hyperf/guzzle": "~3.0.0",
        "sentry/sdk": "^3.0"
    },
    "suggest": {
        "hyperf/guzzle": "Required to use the Guzzle client (~3.0.0).",
        "php-http/guzzle6-adapter": "Required to use the Guzzle transport (^1.0|^2.0).",
        "php-http/guzzle7-adapter": "Required to use the Guzzle transport (^1.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\Sentry\\ConfigProvider"
        }
    }
}
