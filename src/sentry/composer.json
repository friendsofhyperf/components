{
    "name": "friendsofhyperf/sentry",
    "description": "The sentry component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "sentry"
    ],
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Sentry\\": "src/"
        }
    },
    "require": {
        "hyperf/command": "~3.0.0",
        "hyperf/context": "~3.0.16",
        "hyperf/coroutine": "~3.0.0",
        "hyperf/di": "~3.0.0",
        "hyperf/event": "~3.0.0",
        "hyperf/exception-handler": "~3.0.0",
        "hyperf/framework": "~3.0.0",
        "hyperf/http-server": "~3.0.43",
        "hyperf/support": "~3.0.1",
        "hyperf/tappable": "~3.0.0",
        "sentry/sentry": "^4.0"
    },
    "suggest": {
        "hyperf/crontab": "Required to use the crontab event (~3.0.42).",
        "hyperf/database": "Required to use the crontab event (~3.0.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\Sentry\\ConfigProvider"
        }
    },
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    }
}
