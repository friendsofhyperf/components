{
    "name": "friendsofhyperf/encryption",
    "description": "The encryption component for Hyperf.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "hyperf",
        "encryption",
        "swoole",
        "v3.1"
    ],
    "authors": [
        {
            "name": "Huangdijia",
            "email": "huangdijia@gmail.com",
            "homepage": "https://github.com/huangdijia"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components",
        "docs": "https://hyperf.fans",
        "pull-request": "https://github.com/friendsofhyperf/components/pulls"
    },
    "require": {
        "hyperf/context": "~3.1.0",
        "hyperf/contract": "~3.1.0",
        "hyperf/di": "~3.1.0",
        "hyperf/event": "~3.1.0",
        "hyperf/stringable": "~3.1.0",
        "hyperf/tappable": "~3.1.0"
    },
    "suggest": {
        "opis/closure": "For encrypting closures."
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Encryption\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.1-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\Encryption\\ConfigProvider"
        }
    }
}
