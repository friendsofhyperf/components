{
    "name": "friendsofhyperf/notification-easysms",
    "description": "The Symfony mails Channel for friendsofhyperf/notification.",
    "keywords": [
        "hyperf",
        "notification",
        "v3.1",
        "mail"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        },
        {
            "name": "zds"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components",
        "pull-request": "https://github.com/friendsofhyperf/components/pulls"
    },
    "require": {
        "symfony/notifier": "^6.0 | ^7.0",
        "symfony/mailer": "^6.0 | ^7.0",
        "friendsofhyperf/notification": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Notification\\Symfony": "src"
        }
    },
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\Notification\\Symfony\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
