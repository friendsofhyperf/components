{
    "name": "friendsofhyperf/closure-command",
    "description": "The command component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    },
    "require": {
        "hyperf/command": "~3.0.0",
        "hyperf/di": "~3.0.0",
        "hyperf/event": "~3.0.0"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "FriendsOfHyperf\\ClosureCommand\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\ClosureCommand\\ConfigProvider"
        }
    }
}