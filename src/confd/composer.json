{
    "name": "friendsofhyperf/confd",
    "description": "The confd component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "hyperf/command": "~3.0.0",
        "hyperf/di": "~3.0.0",
        "hyperf/event": "~3.0.0",
        "hyperf/framework": "~3.0.0",
        "hyperf/utils": "~3.0.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Confd\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\Confd\\ConfigProvider"
        }
    },
    "suggest": {
        "hyperf/nacos": "For nacos driver.",
        "hyperf/etcd": "For etcd driver."
    }
}