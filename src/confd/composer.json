{
    "name": "friendsofhyperf/confd",
    "description": "The confd component for Hyperf.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "hyperf",
        "v3.1"
    ],
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components",
        "docs": "https://hyperf.fans",
        "pull-request": "https://github.com/friendsofhyperf/components/pulls"
    },
    "require": {
        "hyperf/codec": "~3.2.0",
        "hyperf/collection": "~3.2.0",
        "hyperf/command": "~3.2.0",
        "hyperf/di": "~3.2.0",
        "hyperf/event": "~3.2.0",
        "hyperf/framework": "~3.2.0",
        "hyperf/support": "~3.2.0"
    },
    "suggest": {
        "hyperf/etcd": "For etcd driver.",
        "hyperf/nacos": "For nacos driver."
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Confd\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.1-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\Confd\\ConfigProvider"
        }
    },
    "suggests": {
        "google/protobuf": "Required to use nacos v2 grpc apis.(~3.1.0)",
        "hyperf/grpc": "Required to use nacos v2 grpc apis.(~3.2.0)",
        "hyperf/http2-client": "Required to use nacos v2 grpc apis.(~3.2.0)"
    }
}
