{
    "name": "friendsofhyperf/trigger",
    "description": "The MySQL Trigger component for Hyperf.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "hyperf",
        "v3.2"
    ],
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components",
        "docs": "https://docs.hdj.me",
        "pull-request": "https://github.com/friendsofhyperf/components/pulls"
    },
    "require": {
        "friendsofhyperf/command-signals": "~3.2.0",
        "hyperf/collection": "~3.2.0",
        "hyperf/command": "~3.2.0",
        "hyperf/conditionable": "~3.2.0",
        "hyperf/context": "~3.2.0",
        "hyperf/coordinator": "~3.2.0",
        "hyperf/coroutine": "~3.2.0",
        "hyperf/di": "~3.2.0",
        "hyperf/event": "~3.2.0",
        "hyperf/process": "~3.2.0",
        "hyperf/stdlib": "~3.2.0",
        "hyperf/stringable": "~3.2.0",
        "hyperf/support": "~3.2.0",
        "hyperf/tappable": "~3.2.0",
        "krowinski/php-mysql-replication": "^8.0 || ^9.0",
        "ramsey/uuid": "^4.7"
    },
    "suggest": {
        "hyperf/redis": "Required to use Redis client.(~3.2.0)",
        "hyperf/signal": "Required to use Signal manager.(~3.2.0)"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Trigger\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.1-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\Trigger\\ConfigProvider"
        }
    }
}
