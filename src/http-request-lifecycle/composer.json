{
    "name": "friendsofhyperf/http-request-lifecycle",
    "description": "The http request lifecycle component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
