{
    "name": "friendsofhyperf/tcp-server",
    "description": "Another tcp server component for Hyperf.",
    "keywords": [
        "hyperf",
        "v3.1"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        },
        {
            "name": "zds"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    },
    "require": {
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\TcpServer\\": "src"
        }
    },
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\TcpServer\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    },
    "suggest": {
        "hyperf/async-queue": "Required to use Hyperf\\AsyncQueue\\Job.(~3.1.0)",
        "vlucas/phpdotenv": "Required to use Dotenv.(^5.0)"
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
