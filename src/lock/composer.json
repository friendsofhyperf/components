{
    "name": "friendsofhyperf/lock",
    "description": "The lock component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    },
    "require": {
        "hyperf/config": "~3.0.0",
        "hyperf/utils": "~3.0.0",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Lock\\": "src"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\Lock\\ConfigProvider"
        }
    },
    "suggest": {
        "hyperf/cache": "Require this component for driver 'file'.",
        "hyperf/db-connection": "Require this component for driver 'database'.",
        "hyperf/redis": "Require this component for driver 'redis'."
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}