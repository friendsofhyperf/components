{
    "name": "friendsofhyperf/telescope",
    "description": "An elegant debug assistant for the hyperf framework.",
    "keywords": [
        "swoole",
        "hyperf",
        "debugging"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "hyperf/framework": "~3.0.0",
        "hyperf/support": "~3.0.0",
        "hyperf/event": "~3.0.0",
        "hyperf/view-engine": "~3.0.0",
        "ramsey/uuid": "^4.7",
        "nesbot/carbon": "^2.71",
        "hyperf/command": "~3.0.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\Telescope\\": "src/"
        }
    },
    "suggest": {
        "hyperf/dispatcher": "Required to use BootApplication event.(~3.0.0)",
        "hyperf/command": "Required to use Command annotation.(~3.0.0)",
        "hyperf/database": "Required to use Database annotation.(~3.0.0)",
        "hyperf/db-connection": "Required to use db-connection annotation.(~3.0.0)",
        "hyperf/event": "Required to use Event annotation.(~3.0.0)",
        "hyperf/http-message": "Required to use http-message annotation.(~3.0.0)",
        "guzzlehttp/guzzle": "Required to use Guzzle annotation.(^6.0|^7.0)"
    },
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\Telescope\\ConfigProvider"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    }
}
