{
    "name": "friendsofhyperf/monolog-hook",
    "description": "The monolog hook for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\MonologHook\\": "src/"
        }
    },
    "require": {
        "hyperf/coroutine": "~3.0.0",
        "hyperf/logger": "~3.0.0"
