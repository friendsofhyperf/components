{
    "name": "friendsofhyperf/mysql-grammar-addon",
    "description": "The MySqlGrammar addon for Hyperf.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "dev",
        "hyperf",
        "mysql",
        "grammar",
        "addon",
        "v3.2"
    ],
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components",
        "docs": "https://hyperf.fans",
        "pull-request": "https://github.com/friendsofhyperf/components/pulls"
    },
    "require": {
        "hyperf/database": "~3.2.0",
        "hyperf/di": "~3.2.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\MySqlGrammarAddon\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.1-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\MySqlGrammarAddon\\ConfigProvider"
        }
    }
}
