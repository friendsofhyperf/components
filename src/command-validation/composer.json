{
    "name": "friendsofhyperf/command-validation",
    "description": "The command validation component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "require": {
        "hyperf/command": "~3.1.0",
        "hyperf/context": "~3.1.0",
        "hyperf/di": "~3.1.0",
        "hyperf/stringable": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "hyperf/validation": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\CommandValidation\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\CommandValidation\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    },
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components"
    }
}
