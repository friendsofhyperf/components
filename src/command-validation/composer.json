{
    "name": "friendsofhyperf/command-validation",
    "description": "The command validation component for Hyperf.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "hyperf/command": "~3.0.0",
        "hyperf/context": "~3.0.0",
        "hyperf/di": "~3.0.0",
        "hyperf/validation": "~3.0.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\CommandValidation\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\CommandValidation\\ConfigProvider"
        }
    }
}
