{
    "name": "friendsofhyperf/telescope-elasticsearch",
    "description": "it will allow you to switch from sql database to elasticsearch as driver for your data storage and it will eliminate the deadlock so it makes telescope a ready for production logging system.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "friendsofhyperf",
        "telescope-elasticsearch",
        "telescope",
        "hyperf"
    ],
    "authors": [
        {
            "name": "Saman Jafari",
            "email": "isaimon@me.com",
            "role": "Developer"
        },
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components",
        "docs": "https://hyperf.fans",
        "pull-request": "https://github.com/friendsofhyperf/components/pulls"
    },
    "require": {
        "elasticsearch/elasticsearch": "^8.0 || ^9.0",
        "friendsofhyperf/telescope": "~3.2.0",
        "hyperf/collection": "~3.2.0",
        "hyperf/guzzle": "~3.2.0",
        "hyperf/stringable": "~3.2.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\TelescopeElasticsearch\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.1-dev"
        },
        "laravel": {
            "providers": [
                "FriendsOfHyperf\\TelescopeElasticsearch\\ConfigProvider"
            ]
        }
    }
}
