{
    "name": "friendsofhyperf/exception-event",
    "description": "The exception component for Hyperf.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "hyperf",
        "v3.2"
    ],
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "homepage": "https://github.com/friendsofhyperf/exception-event",
    "support": {
        "issues": "https://github.com/friendsofhyperf/components/issues",
        "source": "https://github.com/friendsofhyperf/components",
        "docs": "https://docs.hdj.me",
        "pull-request": "https://github.com/friendsofhyperf/components/pulls"
    },
    "require": {
        "hyperf/context": "~3.2.0",
        "hyperf/exception-handler": "~3.2.0",
        "hyperf/tappable": "~3.2.0"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\ExceptionEvent\\": "src"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.1-dev"
        },
        "hyperf": {
            "config": "FriendsOfHyperf\\ExceptionEvent\\ConfigProvider"
        }
    }
}
